#include <stdio.h>
#include <stdlib.h>
int main()
{
    int x,y;
    int i,m;
    x=y=1;
    while(1)
    {
        scanf("%d",&x);
        scanf("%d",&y);
        if(x==0&&y==0)
            break;
        else{
            m=1;
          for(i=0;i<y;i++)
          {
              m*=x;
              m=m%1000;
          }
        printf("%d\n",m);
        }
    }
    return 0;
}
